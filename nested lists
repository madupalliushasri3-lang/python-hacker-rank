n = int(input())

students = []

for _ in range(n):
    name = input()
    grade = float(input())
    students.append([name, grade])

# get unique sorted grades
grades = sorted(set([g for _, g in students]))

second_lowest = grades[1]

# collect names with second lowest grade
names = sorted([name for name, g in students if g == second_lowest])

for name in names:
    print(name)
